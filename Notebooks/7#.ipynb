   "source": [
    "import numpy as np\n",
    "\n",
    "# Первая матрица 2x3\n",
    "A = np.random.randint(1, 11, (2, 3))\n",
    "# Вторая матрица 3x2\n",
    "B = np.random.randint(1, 11, (3, 2))\n",
    "print(\"Матрица A:\\n\", A)\n",
    "print(\"Матрица B:\\n\", B)\n",
    "\n",
    "# Матричное умножение\n",
    "product = A @ B  # или np.dot(A, B)\n",
    "print(\"Результат умножения:\\n\", product)"
   ]
  }
