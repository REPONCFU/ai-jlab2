   "source": [
    "import numpy as np\n",
    "\n",
    "# Матрица 4x4 из случайных чисел от 1 до 50\n",
    "A = np.random.randint(1, 51, (4, 4))\n",
    "print(\"Исходная матрица:\\n\", A)\n",
    "\n",
    "# Транспонирование\n",
    "AT = A.T\n",
    "print(\"Транспонированная матрица:\\n\", AT)\n",
    "\n",
    "# След\n",
    "trace = np.trace(A)\n",
    "print(\"След матрицы:\", trace)"
   ]
  }
